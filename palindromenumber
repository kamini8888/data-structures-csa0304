#include<stdio.h>
int main(){
int n,a[50],j,i,flag=0,t;
    printf("Enter size of the array: ");
    scanf("%d", &t);
    while(t--)
    {
    	printf("enter the number of digits");
    	scanf("%d",&n);
  for (i = 0; i <n; i++)
  {
  scanf("%d",&a[i]);
}
    for(i=0,j=n-1;i< (n-1)/2;i++,j--)
	{
    
    	if(a[i] !=a[j]){
    		flag=1;
    		break;
		}
	}
	if(flag==1)
	printf("no");

	else
	printf("yes");
	
}
}
