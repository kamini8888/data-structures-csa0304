#include<string.h>
#include<stdio.h>
int main(){
	int t,flag;
	printf("enter the number of words");
	scanf("%d",&t);
	while(t--){
		int n,i,j,flag;
		char name[100];
		scanf("%s",&name);
		n=strlen(name);
		flag=0;
		for(i=0,j=n-1;i<=(n-1)/2;i++,j--){
			if(name[i]!=name[j]);
			{
		    flag=1;
			break;
			}	

			}
		}
		if(flag==0){
		printf("yes");
	}
		else{
		printf("no");
	}
		
	
	
}
